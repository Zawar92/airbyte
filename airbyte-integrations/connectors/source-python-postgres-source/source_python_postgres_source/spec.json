{
  "documentationUrl": "https://docs.airbyte.io/integrations/sources/postgresuri",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Corona Spec",
    "type": "object",
    "required": ["host", "port", "database", "username", "password"],
    "additionalProperties": false,
    "properties": {
      "host": {
        "type": "string",
        "description": "Host of postgres",
        "pattern": "^[0-9_\\.-]+$",
        "examples": ["0.0.0.0"]
      },
      "port": {
        "type": "string",
        "description": "Port of the database.",
        "pattern": "^[0-9_\\-]+$",
        "examples": ["3000"]
      },
      "database": {
        "type": "string",
        "description": "Name of the database.",
        "pattern": "^[a-z0-9_\\-]+$",
        "examples": ["postgres"]
      },
      "username": {
        "type": "string",
        "description": "Username to use to access the database.",
        "pattern": "^[a-z0-9_\\-]+$",
        "examples": ["postgres"]
      },
      "password": {
        "type": "string",
        "description": "Username to use to access the database.",
        "pattern": "^[a-z0-9_\\-]+$",
        "examples": ["password"]
      }
    }
  }
}
